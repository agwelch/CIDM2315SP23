used AI

public class Professor
{
    private string profName;
    private string classTeach;
    private double salary;

    public Professor(string profName, string classTeach, double salary)
    {
        this.profName = profName;
        this.classTeach = classTeach;
        this.salary = salary;
    }

    public void SetSalary(double salary_amount)
    {
        this.salary = salary_amount;
    }

    public double GetSalary()
    {
        return this.salary;
    }
}

public class Student
{
    private string studentName;
    private string classEnroll;
    private double studentGrade;

    public Student(string studentName, string classEnroll, double studentGrade)
    {
        this.studentName = studentName;
        this.classEnroll = classEnroll;
        this.studentGrade = studentGrade;
    }

    public void SetGrade(double newGrade)
    {
        this.studentGrade = newGrade;
    }

    public double GetGrade()
    {
        return this.studentGrade;
    }
  public class Professor
    {Professor alice = new Professor("Alice", "Java", 9000);
Professor bob = new Professor("Bob", "Math", 8000);

Student lisa = new Student("Lisa", "Java", 90);
Student tom = new Student("Tom", "Math", 80);}

public Professor
{
Console.WriteLine($"Professor {alice.GetProfName()} teaches {alice.GetClassTeach()}, and the salary is: {alice.GetSalary()}");
Console.WriteLine($"Professor {bob.GetProfName()} teaches {bob.GetClassTeach()}, and the salary is: {bob.GetSalary()}");
Console.WriteLine($"Student {lisa.GetStudentName()} enrolls {lisa.GetClassEnroll()}, and the grade is: {lisa.GetGrade()}");
Console.WriteLine($"Student {tom.GetStudentName()} enrolls {tom.GetClassEnroll()}, and the grade is: {tom.GetGrade()}");
}
}
